{
    "name": "Getting Started Example",
    "version": "1.1",
    "description": "Build an Extension!",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
      "browser_action": {
        "default_popup": "main.html"
       },
       "permissions": [
        "contextMenus"
       ]
    
}